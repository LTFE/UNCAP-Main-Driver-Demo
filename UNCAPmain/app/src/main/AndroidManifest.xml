<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uncap.ltfe.uncaptest" >

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="uncap.ltfe.uncaptest.GlobalData">

        <activity
            android:name="uncap.ltfe.uncaptest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="uncap.ltfe.uncaptest.WebViewActivity"
        >
            <intent-filter>
                <action android:name="uncap.ltfe.uncapdrivers.WebViewActivity" />
            </intent-filter>
        </activity>

    </application>


</manifest>
